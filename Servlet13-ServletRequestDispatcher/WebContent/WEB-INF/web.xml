<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Servlet13-ServletRequestDispatcher</display-name>
  
    <servlet>
   <servlet-name>service1</servlet-name>
   <servlet-class>siva.jee.servlet.user.Validations</servlet-class>
   </servlet>
   <servlet-mapping>
   <servlet-name>service1</servlet-name>
   <url-pattern>/validateXml</url-pattern>
   </servlet-mapping>
   
   <servlet>
   <servlet-name>service2</servlet-name>
   <servlet-class>siva.jee.servlet.user.WelcomeUser</servlet-class>
   </servlet>
   <servlet-mapping>
   <servlet-name>service2</servlet-name>
   <url-pattern>/welcomeXml</url-pattern>
   </servlet-mapping>
   
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list> 
</web-app>